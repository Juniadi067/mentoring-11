package aaarrraayyyy;

import java.util.ArrayList;

/**
 *
 * @author Yusuf Hijriah
 */
public class arrayList {

    public static void main(String[] args) {
        ArrayList arr = new ArrayList();
        arr.add("Muhammad Yusuf Hijriah");
        arr.add("Dompu, 27 Maret 2001");
        arr.add("Ilkom");
        arr.add("1901010029");
        arr.add("Laki-Laki");
        arr.add("B");
        arr.add("Islam");
        arr.add("Belum Kawin");
        arr.add("BTN Pemda Gerung");
        arr.add("Mahasiswa");
        System.out.println("_________________________________________");
        System.out.println("|Nama           : " + arr.get(0) + "|");
        System.out.println("_________________________________________");
        System.out.println("|TTL            : " + arr.get(1) + "  |");
        System.out.println("_________________________________________");
        System.out.println("|Prodi          : " + arr.get(2) + "                 |");
        System.out.println("_________________________________________");
        System.out.println("|NIM            : " + arr.get(3) + "            |");
        System.out.println("_________________________________________");
        System.out.println("|Jenis Kelamin  : " + arr.get(4) + "             |");
        System.out.println("_________________________________________");
        System.out.println("|Golongan Darah : " + arr.get(5) + "                     |");
        System.out.println("_________________________________________");
        System.out.println("|Agama          : " + arr.get(6) + "                 |");
        System.out.println("_________________________________________");
        System.out.println("|Status         : " + arr.get(7) + "           |");
        System.out.println("_________________________________________");
        System.out.println("|Alamat         : " + arr.get(8) + "      |");
        System.out.println("_________________________________________");
        System.out.println("|Pekerjaan      : " + arr.get(9) + "             |");
        System.out.println("_________________________________________");
    }

}
